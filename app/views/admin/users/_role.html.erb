<tr>
  <td><%= link_to "Organization #{organization.title}", admin_organization_path(organization.id) %></td>
  <td><%= org_roles[organization].first.role %></td>
  <% org_roles.except!(organization) %>
</tr>
<% roles.each do |role| %>
  <tr>
    <td><%= link_to "Project #{role.project.title}", admin_project_path(role.project_id) %></td>
    <td><%= role.role %></td>
  </tr>
<% end %>
